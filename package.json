{
  "name": "sequel",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "knex --knexfile src/db/knexfile migrate:latest && yarn start:server",
    "start:server": "node build/index.js",
    "build": "tsc",
    "dev": "nodemon src/index.ts",
    "migrate-db": "knex --knexfile src/db/knexfile.ts migrate:make -x ts",
    "migrate-db-apply": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile src/db/knexfile migrate:latest",
    "migrate-db-rollback": "node -r dotenv/config ./node_modules/knex/bin/cli.js --knexfile src/db/knexfile migrate:rollback"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.7",
    "@types/validator": "^13.6.6",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "knex": "^0.95.14",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.9.0",
    "sqlite3": "^5.0.2"
  }
}
